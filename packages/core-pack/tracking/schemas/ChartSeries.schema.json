{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "ChartSeries",
  "type": "object",
  "required": [
    "userId",
    "metricKey",
    "points"
  ],
  "properties": {
    "userId": {
      "type": "string"
    },
    "metricKey": {
      "type": "string",
      "enum": [
        "weightKg",
        "waistCm",
        "sleepQuality",
        "sleepDurationMin",
        "energy",
        "stress",
        "pain",
        "libido",
        "adherenceScore"
      ]
    },
    "points": {
      "type": "array",
      "items": {
        "type": "object",
        "required": [
          "t",
          "v"
        ],
        "properties": {
          "t": {
            "type": "string"
          },
          "v": {
            "type": "number"
          }
        },
        "additionalProperties": false
      }
    },
    "computedAt": {
      "type": "string",
      "format": "date-time"
    }
  },
  "additionalProperties": false
}